config general 'general'
	option verbose '0'

config ss_node 'node'
	option disabled '0'
	option server 'example.com'
	option server_port '41200'
	option password 'example'
	option method 'chacha20-ietf-poly1305'

config ss_local 'redir'
	option disabled '0'
	option protocol 'redir'
	option local_address '::'
	option local_port '10086'
	option mode 'tcp_and_udp'
	option tcp_redir 'tproxy'

config ss_local 'dns'
	option disabled '0'
	option protocol 'dns'
	option local_address '::'
	option local_port '5353'
	option mode 'udp_only'
	option local_dns_address '8.8.8.8'
	option local_dns_port '53'
	option remote_dns_address '8.8.8.8'
	option remote_dns_port '53'
