{
	"luci-app-shadowsocks-rust": {
		"description": "Grant service list access to LuCI app shadowsocks-rust",
		"read": {
			"file": {
				"/usr/bin/sslocal": [ "exec" ]
			},
			"uci": [ "shadowsocks-rust" ]
		},
		"write": {
			"file": {
				"/etc/shadowsocks-rust/*": [ "write" ]
			},
			"uci": [ "shadowsocks-rust" ]
		}
	}
}
